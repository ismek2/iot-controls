<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Color x:Key="OpaqueBackgroundColor">#80000000</Color><!-- !-->
    <Color x:Key="HeaderBackgroundColor">#FF2F3835</Color><!-- !-->
    <Color x:Key="ButtonBackgroundColor">#FF02232F</Color><!-- !-->
    <Color x:Key="WhiteColor">#FFFFFFFF</Color><!-- !-->
    <Color x:Key="BlackColor">#FF000000</Color>
    <Color x:Key="PrimaryAccentColor">#FF446976</Color><!-- !-->
    <Color x:Key="PrimaryAccentColorLight">#FF95DEF1</Color> <!-- !-->
    <Color x:Key="PrimaryAccentColorExtraDark">#FF21434F</Color><!-- !-->
    <Color x:Key="PrimaryAccentColorDark">#FF3A414A</Color><!-- !-->
    <Color x:Key="PrimaryAccentColorAltDark">#FF325C68</Color><!-- !-->
    <Color x:Key="PrimaryAccentColorExtraLight">#FFAEFCFB</Color> <!-- !-->
    
    <Color x:Key="SecondaryAccentColor">#80000000</Color><!-- !-->

